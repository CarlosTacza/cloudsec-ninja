{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "no-referrer-when-downgrade"
                    
                },    
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Download-Options",
                    "value": "noopen"
                },
                {
                    "key": "Cache-Control",
                    "value": "no-store"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "SAMEORIGIN"
                },
                {
                  "key": "Content-Security-Policy",
                  "value": "default-src 'self' *.acloudsecurity.ninja; img-src 'self' data:; script-src 'self' 'unsafe-eval' 'unsafe-inline' connect-src vitals.vercel-insights.com;"
                }
            ]
        }
    ]
}

